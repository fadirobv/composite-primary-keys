{
  "name": "maksimru/composite-primary-keys",
  "description": "Most advanced composite primary keys package with support of: binary columns, queueable, implicit route binding, relations",
  "keywords": [
    "eloquent",
    "composite",
    "binary",
    "keys"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Martianov",
      "email": "maxmtmn@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-curl": "*",
    "illuminate/cache": "^10.0",
    "illuminate/contracts": "^10.0",
    "illuminate/queue": "^10.0",
    "illuminate/redis": "^10.0",
    "illuminate/session": "^10.0",
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19",
    "laravel/framework": "^10.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^6.1",
    "orchestra/testbench": "^8.0",
    "orchestra/testbench-browser-kit": "^8.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MaksimM\\CompositePrimaryKeys\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/database/migrations",
      "tests/database/seeders"
    ],
    "psr-4": {
      "MaksimM\\CompositePrimaryKeys\\Tests\\": "tests/",
      "MaksimM\\CompositePrimaryKeys\\Seeders\\": "tests/database/seeders/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
